<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingDefaultResource">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <size android:height="@dimen/_20sdp" />
            <stroke
                android:width="@dimen/_1sdp"
                android:color="@color/color_seekBar_background" />
            <solid android:color="@color/color_seekBar_background" />
            <corners android:radius="@dimen/dimen_radius_card" />
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip
            android:clipOrientation="horizontal"
            android:gravity="center_horizontal|start">
            <shape android:shape="rectangle">
                <size android:height="@dimen/_20sdp" />
                <solid android:color="@color/color_seekBar_progress" />
                <corners android:radius="@dimen/dimen_radius_card" />
            </shape>
        </clip>
    </item>
</layer-list>

