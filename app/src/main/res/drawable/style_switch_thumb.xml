<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" tools:ignore="MissingDefaultResource">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_switch_thumb" />
            <corners android:radius="@dimen/dimen_radius_card" />
            <size android:width="@dimen/_17sdp" android:height="@dimen/_15sdp" />
        </shape>
    </item>

    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_switch_thumb" />
            <corners android:radius="@dimen/dimen_radius_card" />
            <size android:width="@dimen/_17sdp" android:height="@dimen/_15sdp" />
        </shape>
    </item>
</selector>

